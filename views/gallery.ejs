                    <!-- Pagination -->
                    <% if (totalPages> 1) { %>
                        <div class="insta-pagination">
                            <% if (hasPreviousPage) { %>
                                <a href="/gallery?category=<%= currentCategory %>&page=<%= previousPage %>"
                                    class="insta-page-link">&larr; Previous</a>
                                <% } %>

                                    <% for (let i=1; i <=totalPages; i++) { %>
                                        <a href="/gallery?category=<%= currentCategory %>&page=<%= i %>"
                                            class="insta-page-link <%= currentPage === i ? 'active' : '' %>">
                                            <%= i %>
                                        </a>
                                        <% } %>

                                            <% if (hasNextPage) { %>
                                                <a href="/gallery?category=<%= currentCategory %>&page=<%= nextPage %>"
                                                    class="insta-page-link">Next &rarr;</a>
                                                <% } %>
                        </div>
                        <% } %>
                </div>
                <!-- Gallery Modal -->
                <div id="instaModal" class="insta-modal" style="display: none; opacity: 0;">
                    <span class="insta-close" onclick="closeGallery()">&times;</span>
                    <div class="insta-modal-content">
                        <img id="instaModalImage" class="insta-modal-image" src="" alt="">
                        <div class="insta-modal-caption">
                            <span id="instaCurrentIndex">1</span> / <%= galleryItems.length %> -
                                <span id="instaCaption"></span>
                        </div>
                        <div class="insta-modal-nav">
                            <button onclick="changeImage(-1)">←</button>
                            <button onclick="changeImage(1)">→</button>
                        </div>
                    </div>
                </div>


                <script>
                    document.addEventListener('DOMContentLoaded', function () {
                        let currentImageIndex = 0;
                        const galleryImages = [
                            <% galleryItems.forEach(item => { %>
                            { src: "<%= item.imageUrl %>", title: "<%= item.title %>" },
                            <% }); %>
                        ];

                        function openGallery(index) {
                            currentImageIndex = index;
                            const modal = document.getElementById('instaModal');
                            const modalImg = document.getElementById('instaModalImage');
                            const caption = document.getElementById('instaCaption');
                            const currentIndex = document.getElementById('instaCurrentIndex');

                            if (!modal || !modalImg || !caption || !currentIndex) {
                                console.error('Gallery modal elements not found.');
                                return;
                            }

                            modal.style.display = "flex";
                            setTimeout(() => {
                                modal.style.opacity = 1;
                            }, 10);
                            modalImg.src = galleryImages[currentImageIndex].src;
                            caption.textContent = galleryImages[currentImageIndex].title;
                            currentIndex.textContent = currentImageIndex + 1;
                            document.body.style.overflow = 'hidden';
                        }

                        function closeGallery() {
                            const modal = document.getElementById('instaModal');
                            modal.style.opacity = 0;
                            setTimeout(() => {
                                modal.style.display = "none";
                            }, 300);
                            document.body.style.overflow = 'auto';
                        }

                        function changeImage(direction) {
                            currentImageIndex += direction;
                            if (currentImageIndex >= galleryImages.length) currentImageIndex = 0;
                            if (currentImageIndex < 0) currentImageIndex = galleryImages.length - 1;

                            const modalImg = document.getElementById('instaModalImage');
                            const caption = document.getElementById('instaCaption');
                            const currentIndex = document.getElementById('instaCurrentIndex');

                            modalImg.src = galleryImages[currentImageIndex].src;
                            caption.textContent = galleryImages[currentImageIndex].title;
                            currentIndex.textContent = currentImageIndex + 1;
                        }

                        window.openGallery = openGallery;
                        window.closeGallery = closeGallery;
                        window.changeImage = changeImage;

                        window.onclick = function (event) {
                            const modal = document.getElementById('instaModal');
                            if (event.target === modal) {
                                closeGallery();
                            }
                        }

                        document.addEventListener('keydown', function (event) {
                            const modal = document.getElementById('instaModal');
                            if (modal && modal.style.display === 'flex') {
                                if (event.key === 'Escape') {
                                    closeGallery();
                                } else if (event.key === 'ArrowLeft') {
                                    changeImage(-1);
                                } else if (event.key === 'ArrowRight') {
                                    changeImage(1);
                                }
                            }
                        });

                        window.filterGallery = function (category) {
                            window.location.href = `/gallery?category=${category}&page=1`;
                        }
                    });
                </script>

