                                            <form id="contact-form" class="form w-100 entry-box">
                                              <div class="messages"></div>
                                          
                                              <input style="color: #000;" type="text" name="name" placeholder="Your Name" required />
                                              <input style="color: #000;" type="email" name="email" placeholder="Email" required />
                                              <input style="color: #000;" type="tel" name="phone" placeholder="Phone" required />
                                              <input style="color: #000;" type="text" name="subject" placeholder="Subject" required />
                                              <textarea style="color: #000;" name="message" placeholder="Your Message" rows="5" required></textarea>
                                          
                                              <button type="submit" id="submit-btn">
                                                <span class="submit-text">Send Message</span>
