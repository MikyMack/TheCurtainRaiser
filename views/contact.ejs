                                          <script>
                                            document.getElementById('contact-form').addEventListener('submit', async function (e) {
                                              e.preventDefault();
                                              const form = this;
                                              const submitBtn = form.querySelector('#submit-btn');
                                              const messagesDiv = form.querySelector('.messages');
                                          
                                              submitBtn.disabled = true;
                                              submitBtn.querySelector('.submit-text').style.display = 'none';
                                              submitBtn.querySelector('.loading-text').style.display = 'inline';
                                          
                                              const formData = new FormData(form);
                                              const url = 'https://script.google.com/macros/s/AKfycbyRjR4gIghQoxE8G0EvPbsyfSJE-s3UBh4Q1IioSm99FOSZkFVJZUNfFhOivhHjfUNxAg/exec';
                                          
                                              try {
                                                const response = await fetch(url, {
                                                  method: 'POST',
                                                  body: formData
                                                });
                                                const result = await response.json();
                                          
                                                if (result.status === 'success') {
                                                  messagesDiv.innerHTML = `<div class="alert alert-success">${result.message}</div>`;
                                                  form.reset();
                                                } else {
                                                  throw new Error(result.message || 'Failed to submit');
                                                }
                                              } catch (error) {
                                                messagesDiv.innerHTML = `<div class="alert alert-danger">Error: ${error.message}</div>`;
                                              } finally {
                                                submitBtn.disabled = false;
                                                submitBtn.querySelector('.submit-text').style.display = 'inline';
                                                submitBtn.querySelector('.loading-text').style.display = 'none';
                                              }
                                            });
                                          </script>
                                          
